set foo=%@FILEWRITE[%fhandle,WIN_PYTHON_SNAPSHOT=%WIN_PYTHON_SNAPSHOT]

set foo=%@FILEWRITE[%fhandle,ADDON_GENOMICS=%ADDON_GENOMICS]
set foo=%@FILEWRITE[%fhandle,ADDON_TEXTMINING=%ADDON_TEXTMINING]
set foo=%@FILEWRITE[%fhandle,ADDON_GENOMICS_SNAPSHOT=%ADDON_GENOMICS_SNAPSHOT]
set foo=%@FILEWRITE[%fhandle,ADDON_TEXTMINING_SNAPSHOT=%ADDON_TEXTMINING_SNAPSHOT]

REM # Linux
set foo=%@FILEWRITE[%fhandle,LINUX=%LINUX]
set foo=%@FILEWRITE[%fhandle,LINUX_SNAPSHOT=%LINUX_SNAPSHOT]

set foo=%@FILEWRITE[%fhandle,LINUX_ADDON_GENOMICS=%LINUX_ADDON_GENOMICS]
