pushd
cdd %WEBDIR
cvs -d :sspi:cvso@estelle.fri.uni-lj.si:/CVS -Q update -d -A -- source

cd source
call _pyxtract.bat

call "c:\Program Files\Microsoft Visual Studio 8\vc\vcvarsall.bat"

for %ver in (23 24 25) (
  set PYTHON=%pythonbase%%ver
  set COMPILELOG=%WEBDOWNLOAD\regressionResults\win-compile-%ver.log

  echo *** Compiling for Python %ver
  if exist %COMPILELOG del %COMPILELOG
  start /wait vcexpress orange.sln /build Release /out %COMPILELOG
  flag_on_error
)

popd

